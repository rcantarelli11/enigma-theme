<script>

  {% if section.settings.c_show_read_more %}
      document.addEventListener('DOMContentLoaded', function () {
        const cards = document.querySelectorAll('.devBodytext');

        cards.forEach(card => {
          const content = card.querySelector('.devBodytext p');
          const seeMoreBtn = card.querySelector('.devSeeMore');

          if (content.scrollHeight > content.clientHeight) {
            seeMoreBtn.style.display = 'block';
          }

          seeMoreBtn.addEventListener('click', function () {
            if (content.classList.contains('expanded')) {
              content.classList.remove('expanded');
              seeMoreBtn.textContent = 'Read More';
            } else {
              content.classList.add('expanded');
              seeMoreBtn.textContent = 'Read Less';
            }
          });
        });
      });
    {% endif %}
</script>
  <script>
var swiperOptions = {
loop: true,
  freeMode: true,
  autoplay: {
    delay: 0,
    pauseOnMouseEnter:true,
    disableOnInteraction:false,
  },
  loopAddBlankSlides:true,
  slidesPerView: 1.2,
  speed: 8000,
  grabCursor: true,
  loopAdditionalSlides: 1.2,
   breakpoints: {
        640: {
          slidesPerView: 4,
          spaceBetween: 20,
          loopAdditionalSlides: 4,
        }
       
      },
};

var swiper = new Swiper(".review-swiper", swiperOptions);
</script>
{% schema %}
