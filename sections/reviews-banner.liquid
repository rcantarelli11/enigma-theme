  }

  @media screen and (min-width: 750px) {
    .devMainReviewSection {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
{%- endstyle -%}
<section class="htesti relative text-scheme-text bg-scheme-background overflow-hidden " data-color-scheme="tertiary" style="background-image: url('https://cdn.shopify.com/s/files/1/1618/2767/files/Group_8606.png?v=1707827816');background-size: cover;background-repeat: no-repeat;">

<section class="fitness-banner">
