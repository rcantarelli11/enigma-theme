{%- render 'inline-scripts', script_name: 'visibility-watcher' -%}

<div
  class="password section section--hero"
  data-section-id="{{ section.id }}"
  data-section-type="password"
>
  <div
    class="section-inner section-inner--width-{{ section.settings.width }}"
    style="{% render 'settings-section-padding' %}"
  >
    {% liquid
      if section.settings.enable_text_animation
        assign content_animation_class = ' animation-content'
      endif
    %}
    {%- capture content -%}
      {% for block in section.blocks -%}
        {%- case block.type -%}
          {%- when 'email-form' -%}
            <div class="password__section-block--email-form {{- content_animation_class }}">
              {% render 'newsletter-form', form_id: 'password-newsletter' %}
            </div>

          {%- when 'share' -%}
            <div class="password__section-block--share {{- content_animation_class }}">
              {% render 'social-share',
                enable_os_sharing: block.settings.enable_os_sharing,
                title: shop.name,
                text: shop.description
              %}
            </div>

          {%- when 'social-icons' -%}
            <div class="password__section-block--social-icons {{- content_animation_class }}">
              {% render 'social-icons' %}
            </div>

          {%- when 'password-message' -%}
            {% unless shop.password_message == blank %}
              {% liquid
                capture text_size_class
                  render 'settings-text-size', type: 'text', size: block.settings.text_font_size
                endcapture

                assign text_class = 'password__section-block--password-message content-block content-block--text ff-body rte ' | append: text_size_class

                if section.settings.enable_text_animation
                  assign text_class = text_class | append: ' animation-content'
                endif
              -%}

              <div class="{{ text_class }}" {{ block.shopify_attributes }}>
                {{ shop.password_message }}
              </div>
            {% endunless %}

          {%- else -%}
            {% render 'content-block',
              block: block,
              animate_heading: section.settings.enable_text_animation,
              animate_text: section.settings.enable_text_animation,
              animate_image: section.settings.enable_text_animation,
              animate_divider: section.settings.enable_text_animation,
              forloop: forloop,
              alignment: 'center'
            %}
        {%- endcase -%}
      {%- endfor -%}
    {%- endcapture -%}

    {%- capture footer_content -%}
      {%- if section.settings.show_powered_by_shopify -%}
        <div class="password__content-footer stack {%- if section.settings.enable_text_animation %} animation-content {%- endif %}">
          {% capture shopify %}
            <a
              class="shopify-link"
              href="//www.shopify.com"
              rel="nofollow"
              target="_blank"
              aria-describedby="a11y-new-window-message"
              aria-label="Shopify"
