          {% endcapture %}

          <span class="content-footer__shopify cluster fs-body-75">
            {{ 'general.password_page.powered_by_shopify_html' | t: shopify: shopify }}
          </span>
        </div>
      {%- endif -%}
    {%- endcapture -%}

    {% render 'banner',
      local_settings: section.settings,
      image: section.settings.image,
      mobile_image: section.settings.mobile_image,
      banner_height_type: 'custom',
      banner_width: section.settings.width,
      image_animation: section.settings.image_animation,
      content: content,
      content_max_width: section.settings.content_max_width,
      content_alignment: 'center_center',
      mobile_content_alignment: 'center_center',
      footer_content: footer_content
    %}
