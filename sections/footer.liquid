          "type": "checkbox",
          "id": "collapsible_row_on_mobile",
          "label": "t:sections.footer.blocks.shared.settings.collapsible_row_on_mobile.label",
          "info": "t:sections.footer.blocks.shared.settings.collapsible_row_on_mobile.info",
          "default": false
        },
        {
          "type": "text",
          "id": "heading",
          "label": "t:sections.footer.blocks.shared.settings.heading.label",
          "info": "t:sections.footer.blocks.shared.settings.heading.info",
          "default": "About us"
        },
        {
          "type": "checkbox",
          "id": "show_heading_on_desktop",
          "label": "t:sections.footer.blocks.shared.settings.show_heading_on_desktop.label",
          "info": "t:sections.footer.blocks.shared.settings.show_heading_on_desktop.info",
          "default": true
        },
        {
          "type": "header",
          "content": "t:sections.footer.blocks.image_text_and_socials.settings.content.header.content"
        },
        {
          "type": "inline_richtext",
          "id": "text",
          "label": "t:sections.footer.blocks.shared.settings.text.label",
          "default": "Share contact information, store details, and brand content with your customers."
        },
        {
          "type": "image_picker",
          "id": "image",
          "label": "t:sections.footer.blocks.image_text_and_socials.settings.content.image.label"
        },
        {
          "type": "range",
          "id": "image_width_desktop",
          "min": 50,
          "max": 500,
          "step": 5,
          "unit": "px",
          "label": "t:sections.footer.blocks.image_text_and_socials.settings.content.desktop_image_width.label",
          "info": "t:sections.footer.blocks.image_text_and_socials.settings.content.desktop_image_width.info",
          "default": 100
        },
        {
          "type": "range",
          "id": "image_width_mobile",
          "min": 50,
          "max": 500,
          "step": 5,
          "unit": "px",
          "label": "t:sections.footer.blocks.image_text_and_socials.settings.content.mobile_image_width.label",
          "info": "t:sections.footer.blocks.image_text_and_socials.settings.content.mobile_image_width.info",
          "default": 100
        },
        {
          "type": "select",
          "id": "image_position",
          "label": "t:sections.footer.blocks.image_text_and_socials.settings.content.image_position.label",
          "options": [
            {
              "value": "above-text",
