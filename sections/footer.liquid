          "type": "checkbox",
          "id": "collapsible_row_on_mobile",
          "label": "t:sections.footer.blocks.shared.settings.collapsible_row_on_mobile.label",
          "info": "t:sections.footer.blocks.shared.settings.collapsible_row_on_mobile.info",
          "default": false
        },
        {
          "type": "text",
          "id": "heading",
          "label": "t:sections.footer.blocks.shared.settings.heading.label",
          "info": "t:sections.footer.blocks.shared.settings.heading.info",
          "default": "Links"
        },
        {
          "type": "checkbox",
          "id": "show_heading_on_desktop",
          "label": "t:sections.footer.blocks.shared.settings.show_heading_on_desktop.label",
          "info": "t:sections.footer.blocks.shared.settings.show_heading_on_desktop.info",
          "default": true
        },
        {
          "type": "link_list",
          "id": "menu",
          "label": "t:sections.footer.blocks.links.settings.menu.label",
          "info": "t:sections.footer.blocks.links.settings.menu.info",
          "default": "footer"
        }
      ]
    },
    {
      "type": "newsletter-and-socials",
      "name": "t:sections.footer.blocks.newsletter_and_socials.name",
      "settings": [
        {
          "type": "range",
